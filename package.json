{
  "name": "mpesa-api",
  "version": "2.3.0",
  "description": "An NPM Module built with NodeJs to help you with M-Pesa Daraja API calls.",
  "main": "lib/index",
  "types": "lib/index",
  "homepage": "https://github.com/newtonmunene99/mpesa-api/",
  "scripts": {
    "start": "node lib/index.js",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index",
    "build": "tsc && npm run copy-keys",
    "copy-keys": "ncp src/keys lib/keys",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "keywords": [
    "mpesa",
    "nodejs",
    "node",
    "mobile money",
    "money"
  ],
  "author": "Newton Munene <newtonmunene99@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@newtonmunene99"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newtonmunene99/mpesa-api.git"
  },
  "bugs": {
    "url": "https://github.com/newtonmunene99/mpesa-api/issues"
  }
}
